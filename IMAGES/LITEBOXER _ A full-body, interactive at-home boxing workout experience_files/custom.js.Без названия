$(document).ready(function(){$(".h-tab_content").hide(),$(".h-tab_content:first").show(),$(".h-tab_tab-head li").click(function(){$(".h-tab_content").hide();var t=$(this).attr("rel");$("#"+t).fadeIn(),$(".h-tab_tab-head li").removeClass("active"),$(this).addClass("active")}),$(".pdp-tab").hover(function(){$(".active").addClass("no-border")},function(){$("li").removeClass("no-border")})});
